**{BEGINNING - 7 Math functions}**

require('./lesson.js');

let num1 = Math.floor(Math.random() * 9) + 0;
let num2 = Math.floor(Math.random() * 9) + 0;
let answer = parseInt(prompt("What is "+num1+" x "+num2))
if (answer == num1*num2){
console.log("Thats right!")
}else{
console.log("Wrong, try again.")
}

let dice1 = Math.floor(Math.random() * 6) + 1;
let dice2 = Math.floor(Math.random() * 6) + 1;
let dice3 = Math.floor(Math.random() * 6) + 1;
let counter = 1

console.log(dice1+" "+dice2+" "+dice3)

do{
dice1 = Math.floor(Math.random() * 6) + 1;
dice2 = Math.floor(Math.random() * 6) + 1;
dice3 = Math.floor(Math.random() * 6) + 1;
console.log(dice1,dice2,dice3)
counter = counter+1

}while (dice1 != dice2 || dice2 != dice3 || dice1 != dice3){
console.log("it took "+ counter + " times.")
}

**{END}**

