var name = prompt("What is your name?")     //Words/Text -> String Data
var age = prompt("What is your age?")        //numerical Data
var had_lunch = prompt("Have you had lunch yet?")     //Boolean Data (T/F)

console.log("Hello "+name)
console.log(name+"'s age is "+age)
console.log("Did "+name+" have lunch?")

if (had_lunch == "yes" || "Yes" || "yeah" || "Yeah" || "ya" || "ye" || "Ya" || "Ye"){
console.log("Yes");
} else {
console.log("No");
}

let num  = 10  //Sets num to 10
let num2 = 7   //Sets num to 7

console.log("The number "+num+" multiplied by the number "+num2+" equals "+num*num2)  //Logs num multiplied by num2
console.log("The number "+num+" increased by the number "+num2+" equals "+num+num2)  //Logs num addd with num2
console.log("The number "+num+" decreased by the number "+num2+" equals "+num-num2)  //Logs num subtracted from num2
console.log("The number "+num+" divided by the number "+num2+" equals "+num/num2)  //Logs num divided by num2
console.log("The number "+num+" to the power of "+num2+" equals "+num**num2) //Logs num to the power of num2

**{END}**

