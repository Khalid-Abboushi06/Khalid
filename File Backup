require('./index.js');

let password = 'arnav'
let guess = prompt('Enter the password: ')
let grade = ''
let change_password = prompt("Do you want to change the password? ")

if (change_password == "yes"){
password = prompt("Enter a new password: ")
guess = prompt("Enter the password again: ")
}else{
guess = prompt("Enter the password:")
}

if (guess==password){

grade = prompt("Enter the grade: ")

if (grade<50){

console.log("Not passing")
}  else if (grade>=51&&grade<=69){
console.log("Needs improvement")
}  else if (grade>=70&&grade<=79){
console.log("Meets expectations")
}  else if (grade>=80){
console.log("Exceeding expectations")
}  else{
console.log("Invalid grade, please enter a valid number")
grade = prompt("")
}
}

